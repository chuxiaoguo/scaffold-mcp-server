{
  "name": "scaffold-mcp-server",
  "version": "1.1.12",
  "description": "MCP server for generating frontend scaffolds",
  "main": "dist/index.js",
  "bin": {
    "scaffold-mcp-server": "dist/index.js"
  },
  "type": "module",
  "author": "chuxiaoguo <chuxiaoguo@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chuxiaoguo/scaffold-mcp-server.git"
  },
  "homepage": "https://github.com/chuxiaoguo/scaffold-mcp-server#readme",
  "bugs": {
    "url": "https://github.com/chuxiaoguo/scaffold-mcp-server/issues"
  },
  "files": [
    "dist/",
    "scaffold-template/",
    "README.md",
    "TEMPLATE_GUIDE.md",
    "RELEASE_NOTES.md"
  ],
  "scripts": {
    "build": "tsc && chmod +x dist/index.js && node scripts/copy-templates.cjs",
    "copy-templates": "node scripts/copy-templates.cjs",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "pack:check": "npm pack --dry-run",
    "release": "node scripts/publish.cjs",
    "release:patch": "node scripts/publish.cjs patch",
    "release:minor": "node scripts/publish.cjs minor",
    "release:major": "node scripts/publish.cjs major",
    "release:beta": "node scripts/publish.cjs patch beta",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "pre-publish-check": "node scripts/pre-publish-check.cjs",
    "version:manage": "node scripts/version-manager.cjs",
    "changelog:generate": "node scripts/version-manager.cjs"
  },
  "keywords": [
    "mcp",
    "scaffold",
    "frontend",
    "generator"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "degit": "^2.8.4"
  },
  "bundledDependencies": [
    "@modelcontextprotocol/sdk",
    "degit"
  ],
  "devDependencies": {
    "@types/degit": "^2.8.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
